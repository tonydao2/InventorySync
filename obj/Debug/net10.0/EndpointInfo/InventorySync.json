{
  "openapi": "3.1.1",
  "info": {
    "title": "InventorySync | v1",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://localhost:7072"
    },
    {
      "url": "http://localhost:5218"
    }
  ],
  "paths": {
    "/api/Siteflow/stock": {
      "get": {
        "tags": [
          "Siteflow"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CSVData"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CSVData"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CSVData"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Siteflow/Test": {
      "get": {
        "tags": [
          "Siteflow"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CSVData": {
        "required": [
          "sku",
          "quantity"
        ],
        "type": "object",
        "properties": {
          "sku": {
            "type": "string"
          },
          "quantity": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          }
        }
      }
    }
  },
  "tags": [
    {
      "name": "Siteflow"
    }
  ]
}